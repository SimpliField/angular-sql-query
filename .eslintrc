{
  "extends": "simplifield",
  "env": {
    "es6": true,
    "browser": true,
    "mocha": true
  },
  "globals": {
    "angular": false,
    "inject": false,
    "module": false,
    "require": false,
    "describe": false,
    "beforeEach": false,
    "afterEach": false,
    "before": false,
    "after": false,
    "it": false,
    "sinon": false,
    "expect": false
  },
  "rules": {
    "arrow-parens": 0,
    "indent": 0,
    "keyword-spacing": 0,
    "max-nested-callbacks": 0,
    "no-magic-numbers": 0,
    "no-restricted-properties": 0,
    "operator-linebreak": 0,
    "wrap-iife": 0
  }
}
